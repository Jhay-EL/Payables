<?xml version="1.0" encoding="utf-8"?>
<!-- Backup rules for Android 11 and below (API < 31) -->
<full-backup-content>
    <!-- Exclude all data from backups for security -->
    <exclude domain="sharedpref" path="." />
    <exclude domain="database" path="." />
    <exclude domain="file" path="." />
</full-backup-content>